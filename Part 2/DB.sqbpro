SELECT b.brandCode, b.name, b.barcode, COUNT(r._id) as receipt_count
FROM brands as b
left join items as i on b.barcode = i.barcode
LEFT JOIN receipts as r ON i._id = r._id
WHERE r.userId IN (SELECT _id FROM users WHERE (strftime('%Y',createdDate)) = 2021 AND (strftime('%M',createdDate)) IN (1, 2, 3)
OR (strftime('%Y',createdDate)) = 2020 AND (strftime('%M',createdDate)) IN (9, 10, 11, 12)) 
GROUP BY b.brandCode 
ORDER BY receipt_count DESC;</sql><current_tab id="0"/></tab_sql></sqlb_project>
